{
    "buildCommand": "npm run build:all",
    "installCommand": "npm install",
    "devCommand": "npm run dev",
    "functions": {
        "api/index.js": {
            "maxDuration": 10
        }
    },
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/api/index"
        }
    ]
}
