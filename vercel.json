{
    "buildCommand": "npm run build:all",
    "outputDirectory": "dist",
    "installCommand": "npm install",
    "devCommand": "npm run dev",
    "framework": null,
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/api/index"
        }
    ],
    "functions": {
        "api/index.js": {
            "maxDuration": 10
        }
    }
}
